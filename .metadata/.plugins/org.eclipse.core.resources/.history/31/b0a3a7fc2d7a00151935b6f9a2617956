package Enteties;

import java.util.ArrayList;

import Items.Item;

public class Player extends Entity{
	private String name;
	private ArrayList<Item> inventory;

	public Player(int health, int attackDamage, String name) {
		super(health, attackDamage);
		this.name = name;
	}
	
	public String getName(){
		return this.name;
	}

}
